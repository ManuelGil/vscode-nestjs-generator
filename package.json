  "icon": "icon.png",
  "license": "MIT",
  "publisher": "imgildev",
  "author": {
    "name": "Manuel Gil",
    "url": "https://github.com/ManuelGil"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/manuelgil"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "autocomplete-functionality",
    "autocomplete",
    "boilerplate",
    "commands",
    "express",
    "extension",
    "generator",
    "grammars-utility",
    "grammars",
    "javascript",
    "js",
    "nest",
    "nestjs",
    "node",
