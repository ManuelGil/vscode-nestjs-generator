{
  "name": "vscode-nestjs-generator",
  "displayName": "NestJS File Generator for VSCode",
  "description": "Generate NestJS files from the command palette or explorer context menu (right click). It also provides a terminal to start the server in different modes (dev, debug, prod). It is based on the NestJS CLI.",
  "version": "2.6.0",
  "icon": "icon.png",
  "license": "MIT",
  "publisher": "imgildev",
  "author": {
    "name": "Manuel Gil",
    "url": "https://github.com/ManuelGil"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/manuelgil"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "autocomplete-functionality",
    "autocomplete",
    "boilerplate",
    "commands",
    "express",
    "extension",
    "generator",
    "grammars-utility",
    "grammars",
    "javascript",
    "js",
    "nest",
    "nestjs",
    "node",
    "nodejs",
    "ts",
    "typescript",
    "vscode-extension",
    "vscode"
  ],
  "homepage": "https://github.com/ManuelGil/vscode-nestjs-generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/ManuelGil/vscode-nestjs-generator"
  },
  "bugs": {
    "url": "https://github.com/ManuelGil/vscode-nestjs-generator/issues"
  },
  "extensionPack": [
    "imgildev.vscode-nestjs-snippets-extension"
  ],
  "activationEvents": [
    "workspaceContains:**/nest-cli.json"
  ],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "configuration": {
      "title": "NestJS File Generator",
      "properties": {
        "nestjs.files.include": {
          "type": "array",
          "default": [
            "ts"
          ],
          "scope": "resource",
          "description": "%nestjs.files.include%"
        },
        "nestjs.files.exclude": {
          "type": "array",
          "default": [
            "**/node_modules/**",
            "**/dist/**",
            "**/out/**",
            "**/build/**"
          ],
          "scope": "resource",
          "description": "%nestjs.files.exclude%"
        },
        "nestjs.files.watch": {
          "type": "array",
          "default": [
            "controllers",
            "dtos",
            "services"
          ],
          "scope": "resource",
          "description": "%nestjs.files.watch%"
        },
        "nestjs.files.showPath": {
          "type": "boolean",
          "default": true,
          "scope": "resource",
          "description": "%nestjs.files.showPath%"
        },
        "nestjs.submenu.customCommands": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "%nestjs.submenu.customCommands.name%"
              },
              "command": {
                "type": "string",
                "description": "%nestjs.submenu.customCommands.command%"
              },
              "args": {
                "type": "string",
                "description": "%nestjs.submenu.customCommands.args%"
              }
            }
          },
          "default": [
            {
              "name": "Template 1",
              "command": "nest g co",
              "args": "--flat"
            },
            {
              "name": "Template 2",
              "command": "nest g co",
              "args": "--no-flat"
            }
          ],
          "scope": "resource",
          "description": "%nestjs.submenu.customCommands%"
        },
        "nestjs.submenu.activateItem": {
          "type": "object",
          "properties": {
            "file": {
              "type": "object",
              "properties": {
                "class": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.class%"
                },
                "controller": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.controller%"
                },
                "decorator": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.decorator%"
                },
                "dto": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.dto%"
                },
                "exception": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.exception%"
                },
                "exceptionFilter": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.exceptionFilter%"
                },
                "filter": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.filter%"
                },
                "gateway": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.gateway%"
                },
                "guard": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.guard%"
                },
                "interceptor": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.interceptor%"
                },
                "interface": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.interface%"
                },
                "jwtGuard": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.jwtGuard%"
                },
                "jwtStrategy": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.jwtStrategy%"
                },
                "middleware": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.middleware%"
                },
                "logger": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.logger%"
                },
                "module": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.module%"
                },
                "pipe": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.pipe%"
                },
                "provider": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.provider%"
                },
                "resolver": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.resolver%"
                },
                "service": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.service%"
                },
                "test": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.file.test%"
                }
              }
            },
            "terminal": {
              "type": "object",
              "properties": {
                "controller": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.controller%"
                },
                "gateway": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.gateway%"
                },
                "library": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.library%"
                },
                "module": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.module%"
                },
                "provider": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.provider%"
                },
                "resolver": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.resolver%"
                },
                "resource": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.resource%"
                },
                "service": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.service%"
                },
                "custom": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.custom%"
                },
                "start": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.start%"
                },
                "startDev": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.startDev%"
                },
                "startDebug": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.startDebug%"
                },
                "startProd": {
                  "type": "boolean",
                  "default": true,
                  "scope": "resource",
                  "description": "%nestjs.submenu.activateItem.terminal.startProd%"
                }
              }
            }
          },
          "default": {
            "file": {
              "class": true,
              "controller": true,
              "decorator": true,
              "dto": true,
              "exception": true,
              "exceptionFilter": true,
              "filter": true,
              "gateway": true,
              "guard": true,
              "interceptor": true,
              "interface": true,
              "jwtGuard": true,
              "jwtStrategy": true,
              "middleware": true,
              "logger": true,
              "module": true,
              "pipe": true,
              "provider": true,
              "resolver": true,
              "service": true,
              "test": true
            },
            "terminal": {
              "controller": true,
              "gateway": true,
              "library": true,
              "module": true,
              "provider": true,
              "resolver": true,
              "resource": true,
              "service": true,
              "custom": true,
              "start": true,
              "startDev": true,
              "startDebug": true,
              "startProd": true
            }
          },
          "scope": "resource",
          "description": "%nestjs.submenu.activateItem%"
        },
        "nestjs.files.autoImport": {
          "type": "boolean",
          "default": true,
          "scope": "resource",
          "description": "%nestjs.files.autoImport%"
        },
        "nestjs.files.orm": {
          "type": "string",
          "default": "typeorm",
          "enum": [
            "mikroorm",
            "mongoose",
            "sequelize",
            "typeorm"
          ],
          "scope": "resource",
          "description": "%nestjs.files.orm%"
        }
      }
    },
    "commands": [
